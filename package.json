{
  "name": "StarkNet-indexer",
  "version": "1.0.0",
  "description": "Opensource StarkNet indexer using apollo server, typescript, prisma & graphql",
  "main": "server.js",
  "scripts": {
    "start": "yarn build-ts && npm run serve",
    "dev": "nodemon app/server.ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "build-ts": "prisma generate && tsc",
    "watch-ts": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Tyscript",
    "Graphql",
    "node",
    "javascript",
    "prisma"
  ],
  "author": "Pondering Democritus",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "nodemon": "^2.0.14",
    "prisma": "3.15.0",
    "ts-node": "^10.9.1",
    "typegraphql-prisma": "^0.20.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@prisma/client": "3.15.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/node-fetch": "^2.6.1",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.4.0",
    "class-validator": "^0.13.1",
    "ethers": "^5.6.2",
    "express": "^4.17.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "node-fetch": "2",
    "prisma-docs-generator": "^0.5.0",
    "reflect-metadata": "^0.1.13",
    "starknet": "^3.9.0",
    "tslib": "^2.3.1",
    "type-graphql": "^1.1.1"
  },
  "peerDependencies": {
    "graphql": "^15.8.0"
  }
}
